{
  "expo": {
    "name": "EcommerceEarn",
    "slug": "ecommerceearn-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#6366f1",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ecommerceearn.app"
    },
    "android": {
      "package": "com.ecommerceearn.app"
    },
    "scheme": "ecommerceearn",
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
